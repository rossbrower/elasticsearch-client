{
  "version": "5.0.0-beta1",
  "title": "ElasticsearchClient",
  "description": "Unofficial low-level .Net client for Elasticsearch built from the REST API spec and HttpClient.",  
  "authors": ["Ross Brower"],
    "packOptions": {
    "projectUrl": "https://github.com/rossbrower/elasticsearch-client",
    "tags": [ "elasticsearch" ],
    "licenseUrl": "https://raw.github.com/rossbrower/elasticsearch-client/master/LICENSE.md",
    "repository": {
      "type": "git",
      "url": "git://github.com/rossbrower/elasticsearch-client"
    }
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_1",
          "PORTABLE"
        ]
      },
      "dependencies": {
        "System.Threading": "4.0.11",
        "System.Threading.Tasks": "4.0.11",
        "System.Net.Http": "4.1.0",
        "System.IO": "4.1.0",
        "System.Linq": "4.1.0",
        "System.Runtime.Extensions": "4.1.0"
      }
    }
  }
}